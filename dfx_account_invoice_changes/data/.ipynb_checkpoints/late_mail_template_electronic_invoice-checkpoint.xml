<odoo>
    <data noupdate="0">
        <record id="FE_late_email_template" model="mail.template">
            <field name="name">Invoice Late Example e-mail template</field>
            <field name="email_from">${object.company_id and object.company_id.email or ''}</field>
            <field name="subject">Factura ${object.name}</field>
            <field name="email_to">${object.email|safe}</field>
            <field name="lang">${object.lang}</field>
            <field name="model_id" ref="dfx_account_invoice_changes.model_account_move"/>
            <field name="auto_delete" eval="True"/>
            <field name="body_html">
                <![CDATA[
                 % set access_action = object.get_access_action()
                 % set access_url = object.get_mail_url()
          <p>La factura con el identificador ${(object.name)} a procedido a ser anulada, porque sobrepasó los 3 días hábiles.<br/></p>
              <p>No conteste este correo</p>
              <p>Saludos,</p><br/>
              ${(object.company_id.name)}
        ]]>
            </field>
        </record>
    </data>
</odoo>