<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="account_move_form_inherit_custom" model="ir.ui.view">
        <field name="name">account.move.form.inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="priority" eval="98"/>
        <field name="arch" type="xml">
            <xpath expr="//h1[hasclass('mt0')]" position="after">
                <h2>
                    <label for="project_title" class="oe_edit_only"/>
                    <field name="project_title" class="oe_inline"/>
                </h2>
            </xpath>
        </field>
    </record>
    
</odoo>