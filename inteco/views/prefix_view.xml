<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="prefix_form_view" model="ir.ui.view">
        <field name="name">prefix.form.view</field>
        <field name="model">inteco.prefix</field>
        <field name="arch" type="xml">
            <form string="Prefixes">
                <div class="oe_title">
                    <label for="name" class="oe_edit_only"/>
                    <h1><field name="name" placeholder="e.g. INTE/ISO"/></h1>
                </div>
                <group>
                    <field name="type" widget="radio"/>
                </group>
            </form>
        </field>
    </record>
</odoo>
